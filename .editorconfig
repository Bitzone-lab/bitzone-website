# EditorConfig is awesome: https://EditorConfig.org
# https://stackoverflow.com/questions/41518677/how-to-apply-editorconfig-to-existing-project-in-intellige

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
